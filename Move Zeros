class Solution {
    public void moveZeroes(int[] nums) {
        int nonZerosIndex = 0;
        for (int i=0; i<nums.length; i++){
            if (nums[i] != 0){
                nums[nonZerosIndex++] = nums[i];
            }
        }
        for (int i=nonZerosIndex; i<nums.length; i++){
            nums[i] = 0;
        }
    }
}
