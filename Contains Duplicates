class Solution {
    fun containsDuplicate(nums: IntArray): Boolean {
        if (nums.size <= 1){
            return false
        }

        val hMap = mutableSetOf<Int>()

        for (num in nums) {
            if (num in hMap) {
                return true
            } else {
                hMap.add(num)
            }
        }
        return false
    }
}
